<Window x:Class="LeThanhPhongWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeThanhPhongWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        >
    <Grid>
        <TextBox HorizontalAlignment="Center" Margin="0,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="168" Height="31" />
        <PasswordBox HorizontalAlignment="Center" VerticalAlignment="Center" Width="168" Height="31" />
        <TextBlock HorizontalAlignment="Center" Margin="0,88,0,0" TextWrapping="Wrap" FontSize="18pt" TextAlignment="Center" FontWeight="Bold" Text="FU Car Reting Management" VerticalAlignment="Top" Height="44" Width="362"/>
        <Button Content="Login" HorizontalAlignment="Center" Margin="0,260,0,0" VerticalAlignment="Top" Padding="15px 5px" Click="Button_Click"/>
        <Label Content="Email" HorizontalAlignment="Left" Margin="250,169,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.704,-0.301"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="250,207,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.704,-0.301"/>

    </Grid>
</Window>
